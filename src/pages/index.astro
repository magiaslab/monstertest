<link href="/src/css/lightbox.css" rel="stylesheet" />
<Main title={Title}>
<Navbar/>



<div class="grid grid-cols-2 md:grid-cols-3 gap-4">
    <div>
        <a href="/src/assets/immaginacamillo.jpg" data-lightbox="image-2"><Image src={Camillo} alt="Camillo Bianchi" class="h-auto max-w-full rounded-lg" /></a>
    </div>
    <div>
        <Image src={Mens} alt="Camillo Bianchi" class="h-auto max-w-full rounded-lg"/>
    </div>
    <div>
       <Cldimage
       width="100%"
       height="100%"
       src="SVB/mensana/434127589_7638278046223493_5654075145264052061_n_rnheas"
       alt="Mens Sana"
       class="h-auto max-w-full rounded-lg"
       />
    </div>
    <div>
        <img class="h-auto max-w-full rounded-lg" src="https://flowbite.s3.amazonaws.com/docs/gallery/square/image-3.jpg" alt="" data-lightbox="image-1" data-title="My caption">
    </div>
